<div class="container">
  <p id="notice"><%= notice %></p>

  <h1>Quotes</h1>

  <table>
    <thead>
      <tr>
        <th>First name</th>
        <th>Last name</th>
        <th>Email</th>
        <th>Phone number</th>
        <th>Type</th>
        <th>Organization</th>
        <th>Vehicule type</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @quotes.each do |quote| %>
        <tr>
          <td><%= quote.first_name %></td>
          <td><%= quote.last_name %></td>
          <td><%= quote.email %></td>
          <td><%= quote.phone_number %></td>
          <td><%= quote.type %></td>
          <td><%= quote.organization %></td>
          <td><%= quote.vehicule_type %></td>
          <td><%= link_to 'Show', quote_path(quote) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Quote', new_quote_path %>
</div>
